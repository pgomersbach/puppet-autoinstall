<%= server_to_mon %>,Current load,check_snmp_load.pl -H <%= server_to_mon %> -C public -2 -w 70 -c 90 -f 
<%= server_to_mon %>,Root Partition,check_snmp_storage.pl -H <%= server_to_mon %> -2 -C public -T pu -w 90% -c 95% -m / -f -r 
<%= server_to_mon %>,Eth0 load,check_snmp_int.pl -H <%= server_to_mon %> -C public -n eth0 -w 0,0 -c 0,0 -f 
<%= server_to_mon %>,Disk Usage,check_disk.pl -H <%= server_to_mon %> -u supervisor
<%= server_to_mon %>,Parent slave,check_myslave.sh 
